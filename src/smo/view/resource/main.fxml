                <Font name="Courier" size="24.0"/>
            </font>
        </Label>
        <Label fx:id="l_name" layoutX="1400.0" layoutY="800.0" text="Лялин Данил"/>
        <Label fx:id="l_group" layoutX="1400.0" layoutY="820.0" text="3530904/80102"/>

        <Button fx:id="b_AutoMode" layoutX="700.0" layoutY="380.0" mnemonicParsing="false" text="Автоматический режим"/>
        <Button fx:id="b_StepMode" layoutX="700.0" layoutY="480.0" mnemonicParsing="false" text="Пошаговый режим"/>
        <Button fx:id="b_SetParameters" layoutX="700.0" layoutY="580.0" mnemonicParsing="false"
